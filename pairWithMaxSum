class Solution
{
    public:
    long long pairWithMaxSum(long long arr[], long long N)
    {
        long long max=0;
        for(int i=0; i<N-1; i++)
        {
            if(arr[i]+arr[i+1]>max)
            {
                max=arr[i]+arr[i+1];
            }
        }
        return max;
        
    }
};
